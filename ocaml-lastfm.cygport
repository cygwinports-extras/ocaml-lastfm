inherit ocaml

NAME="ocaml-lastfm"
VERSION=0.3.0
RELEASE=3
CATEGORY="OCaml"
SUMMARY="OCaml Last.fm library"
DESCRIPTION="This package contains an O'Caml interface for Lastfm API."
HOMEPAGE="http://savonet.sourceforge.net/"
SRC_URI="mirror://sourceforge/savonet/${NAME}-${VERSION}.tar.gz"

DIFF_EXCLUDES="Makefile.*"

src_compile() {
	lndirs
	cd ${B}
	cygconf
	cygmake -j1
}
